{
  "name": "micro-mud-user-service",
  "version": "1.0.0",
  "description": "User service for the micro MUD.",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "security-check": "npm audit",
    "open:src": "babel-node ./build_scripts/dev_server.js",
    "lint": "esw src build_scripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress build_scripts/test_setup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirai-wren/micro-mud-user-service.git"
  },
  "author": "Mirai Wren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mirai-wren/micro-mud-user-service/issues"
  },
  "homepage": "https://github.com/mirai-wren/micro-mud-user-service#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-watch": "^3.1.5",
    "express": "^4.16.3",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {}
}
